<%= render "layouts/errors", :object => @review %>

<%= form_for [@meat, @review] do |f| %>
  <div class="form-group">
    <%= f.label :rating %>
    <%= f.text_field :rating, class: "form-control"  %>
  </div>
  <div class="form-group">
    <%= f.label :comment %>
    <%= f.text_field :comment, class: "form-control" %>
  </div>
  <%= f.submit %>
<% end %>
